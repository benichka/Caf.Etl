{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "definitions": {
    "cosmosEntity": {
      "type": "object",
      "properties": {
        "partitionKey": {
          "type": "string"
        },
        "id": {
          "type": "string"
        },
        "type": {
          "type": "string"
        },
        "schema": {
          "type": "string"
        }
      },
      "required": [
        "partitionKey",
        "id",
        "type",
        "schema"
      ]
    },
    "cosmosInternal": {
      "type": "object",
      "properties": {
        "_rid": {
          "type": "string"
        },
        "_self": {
          "type": "string"
        },
        "_etag": {
          "type": "string"
        },
        "_attachments": {
          "type": "string"
        },
        "_ts": {
          "type": "integer"
        }
      }
    },
    "areaOfInterest": {
      "type": "string",
      "enum": [
        "CookEast",
        "CookWest",
        "BoydNorth",
        "BoydSouth",
        "Cook",
        "Boyd",
        "Caf",
        "Ltar"
      ]
    },
    "location": {
      "$ref": "geojson.json#"
    },
    "physicalQuantities": {
      "items": {
        "properties": {
          "value": {
            "type": "number"
          },
          "unit": {
            "type": "string"
          },
          "submissionDateTime": {
            "type": "string",
            "format": "date-time"
          },
          "sourceId": {
            "type": "string"
          },
          "qualityControlId": {
            "type": "string"
          }
        },
        "required": [
          "value",
          "unit",
          "submissionDateTime",
          "sourceId"
        ],
        "type": "object"
      },
      "type": "array"
    },
    "personContact": {
      "type": "object",
      "properties": {
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "phoneNumber": {
          "type": "string",
          "pattern": "^(\\([0-9]{3}\\))?[0-9]{3}-[0-9]{4}$"
        },
        "email": {
          "type": "string",
          "format": "email"
        },
        "affiliatedOrganization": {
          "type": "string"
        },
        "positionTitle": {
          "type": "string"
        }
      },
      "required": [
        "firstName",
        "lastName",
        "email",
        "affiliatedOrganization",
        "positionTitle"
      ]
    }
  },
  "id": "http://files.cafltar.org/data/schema/documentDb/v2/common.json#"
}